{
	"name": "facit",
	"version": "2.2.0",
	"private": true,
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"audit": "yarn audit --groups dependencies",
		"outdated": "yarn outdated",
		"serve": "serve -s build",
